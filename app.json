{
  "expo": {
    "name": "unirefund-mobile",
    "slug": "unirefund-mobile",
    "version": "1.0.0",
    "scheme": "unirefund-mobile",
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "minSdkVersion": 26
          }
        }
      ],
      "expo-router",
      [
        "expo-dev-launcher",
        {
          "launchMode": "most-recent"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": true
        }
      ],
      [
        "react-native-vision-camera",
        {
          "cameraPermissionText": "$(PRODUCT_NAME) needs access to your Camera.",
          "enableMicrophonePermission": true,
          "microphonePermissionText": "$(PRODUCT_NAME) needs access to your Microphone."
        }
      ],

      [
        "expo-splash-screen",
        {
          "backgroundColor": "#232323",
          "image": "./assets/unirefund.png",
          "imageWidth": 200
        }
      ],
      "expo-secure-store",
      "react-native-vision-camera"
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./assets/unirefund.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/unirefund.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.unirefund.mobile",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        }
      }
    },
    "android": {
      "package": "com.unirefund.mobile",
      "adaptiveIcon": {
        "foregroundImage": "./assets/unirefund.png",
        "backgroundColor": "#ffffff"
      }
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "024912fd-2a4b-4985-a77c-4320409eb075"
      }
    }
  }
}
