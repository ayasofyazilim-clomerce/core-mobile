{
  "expo": {
    "name": "unirefund-mobile",
    "slug": "unirefund-mobile",
    "version": "1.0.0",
    "scheme": "unirefund-mobile",

    "plugins": [
      "expo-router",
      [
        "expo-dev-launcher",
        {
          "launchMode": "most-recent"
        }
      ],
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#232323",
          "image": "./assets/unirefund.png",

          "imageWidth": 200
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./assets/unirefund.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/unirefund.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.unirefund.unirefundmobile",
      "adaptiveIcon": {
        "foregroundImage": "./assets/unirefund.png",
        "backgroundColor": "#ffffff"
      }
    }
  }
}
