{
  "expo": {
    "name": "unirefund-mobile",
    "slug": "unirefund-mobile",
    "version": "1.0.0",
    "scheme": "unirefund-mobile",
    "plugins": ["react-native-app-clip"],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./assets/unirefund.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/unirefund.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.unirefund.mobile",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        }
      }
    },
    "android": {
      "package": "com.unirefund.mobile",
      "adaptiveIcon": {
        "foregroundImage": "./assets/unirefund.png",
        "backgroundColor": "#ffffff"
      }
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "024912fd-2a4b-4985-a77c-4320409eb075"
      }
    }
  }
}
